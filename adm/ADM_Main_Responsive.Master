                <div>
                    <h2 style="margin: 0; text-align: center; color:white;">
                        <span class="navbar navbar-expand-md bg-dark">Switch Company
                        </span>
                    </h2>
                    <table class="table-sm" style="width: 800px">
                        <tr>
                            <td style="width: 40%" rowspan="2">

                                <asp:Image runat="server" CssClass="img-thumbnail" ImageUrl="~/Images/logo/vertical logo on light bg.svg" Width="100%" />

                            </td>
                            <td style="vertical-align: top">
                                <table class="table-sm" style="width: 100%">
                                    <tr>
                                        <td style="font-size: large; font-weight: bold">Select Company:
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <telerik:RadComboBox ID="cboCompany" runat="server" DataSourceID="SqlDataSourceCompany" ToolTip="Company" ZIndex="50001" Width="100%" Skin="Material"
                                                DataTextField="Name" DataValueField="companyId" Height="230px">
                                            </telerik:RadComboBox>
                                        </td>
                                    </tr>
