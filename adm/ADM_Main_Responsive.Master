            var allnodes = nav.get_allNodes() 
            allnodes.forEach(node => node.collapse());
            //nav.collapseMenuButton();
        }

        function OnClientNodeExpandingMenu3(sender, eventArgs) {
            var nav = $find("<%= RadNavigation2.ClientID %>");
